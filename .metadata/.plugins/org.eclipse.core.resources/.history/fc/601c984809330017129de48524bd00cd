		try{
			//JDBCドライバのロード
			Class.forName("com.mysql.jdbc.Driver");
			
			//データベースと接続
			con = DriveManager.getConnection();
			
		}catch{
			
		}catch{
			
		}finally{
			
		}
	}
}
